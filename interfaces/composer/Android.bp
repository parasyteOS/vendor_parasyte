aidl_interface {
    name: "vendor.parasyte.composer",
    system_ext_specific: true,
    srcs: [ "vendor/parasyte/composer/*.aidl" ],
    imports: [
        "android.hardware.graphics.common-V5",
    ],

    unstable: true,
    // cheat around setting a version
    owner: "parasyteOS",
    backend: {
        java: {
            enabled: true,
            platform_apis: true,
        },
        cpp: {
            enabled: false,
        },
        ndk: {
            enabled: true,
        },
    },
}
